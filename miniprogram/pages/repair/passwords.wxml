<!--miniprogram/pages/homePage/password.wxml-->
<!-- <text>miniprogram/pages/homePage/password.wxml</text> -->
<view class='imagesize'>
<image src='/images/password.png' bindload="imageLoad"
style="width:{{imgwidth}}rpx; height:{{imgheight }}rpx;"  class='in-image'></image></view>
<!--启动密码-->
<view class="cu-form-group">
  <view class="title">启动密码</view>
  <input name="password" placeholder="请输入启动密码" password="true" bindchange="passwordChange"></input>
</view>

<!--提交按钮-->
<view class="padding flex flex-direction">
    <button class="cu-btn bg-green margin-tb-sm lg" bindtap='submit' >确认</button>
</view>
